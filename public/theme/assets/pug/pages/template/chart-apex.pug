- var theme_customizer      = true;
- var apex                  = true;
- var page_apex             = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
  
 
//- all mixin  
include ../../mixin/mixin

doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-6
                  h4
                    | Apex Chart

                .col-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Charts
                    li.breadcrumb-item.active Apex Chart
          // Container-fluid starts
          .container-fluid
            .row
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4 
                      | Basic Area Chart
                  .card-body
                    #basic-apex
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Area Spaline Chart 
                  .card-body
                    #area-spaline
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Bar chart
                  .card-body
                    #basic-bar
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Column Chart 
                  .card-body
                    #column-chart
              .col-sm-12.col-xl-6.box-col-12
                .card
                  .card-header
                    h4 
                      | 3d Bubble Chart 
                  .card-body
                    #chart-bubble

              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Stepline Chart 
                  .card-body
                    #stepline
              .col-sm-12.col-xl-12.box-col-6
                .card
                  .card-header
                    h4
                      | Column Chart
                  .card-body
                    #annotationchart
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Pie Chart 
                  .card-body.apex-chart
                    #piechart
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Donut Chart
                  .card-body.apex-chart
                    #donutchart
              .col-sm-12.col-xl-12.box-col-12
                .card
                  .card-header
                    h4 Mixed Chart
                  .card-body
                    #mixedchart                        
              .col-sm-12.col-xl-12.box-col-12
                .card
                  .card-header
                    h4
                      | Candlestick Chart 
                  .card-body
                    #candlestick
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Radar Chart 
                  .card-body
                    #radarchart
              .col-sm-12.col-xl-6.box-col-6
                .card
                  .card-header
                    h4
                      | Radial Bar Chart
                  .card-body
                    #circlechart
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
