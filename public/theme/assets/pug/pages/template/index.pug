- var theme_customizer      = true;
- var echart                = true;
- var echart_pie            = true;
- var sidebar               = true;
- var apexmoment            = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
- var apex                  = true;
- var dashboard_3           = true; 
- var date_picker           = true; 
  
 
//- all mixin  
include ../../mixin/mixin

doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-6
                  h4
                    | Default
                .col-6 
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Dashboard
                    li.breadcrumb-item.active Default
          // Container-fluid starts
          .container-fluid 
            .row.size-column
              .col-xl-8.col-md-12.box-col-12
                .card
                  .card-header.sales-chart.card-no-border
                    h4 Revenue Growth  
                    div.sales-chart-dropdown 
                      ul.balance-data
                        li 
                          span.circle.bg-warning 
                          span.ms-1.f-w-400 Marketing Sale 
                        li   
                          span.circle.bg-primary 
                          span.ms-1.f-w-400 Online Sale
                      div.sales-chart-dropdown-select
                        .card-header-right-icon.online-store 
                          .dropdown 
                            button#dropdownMenuButtonToggle.btn.dropdown-toggle.dropdown-toggle-store(type='button', data-bs-toggle='dropdown', aria-expanded='false')
                              | This Year
                            .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButtonToggle')
                              span.dropdown-item Last Month
                              span.dropdown-item Last Week
                              span.dropdown-item Today   
                  .card-body.pt-0 
                    .row 
                      .col-xxl-8.col-xl-12
                        .revenuegrowth
                          #revenuegrowth(class="revenuegrowth-chart")
                      .col-xxl-4.col-xl-4(class="d-xxl-block d-none ")
                        div(class="revenuegrowth-details") 
                         div(class="growth-details")
                           span(class="f-light f-12  text-uppercase") Total Sales
                           h4(class="f-w-500 mb-2") $56.265.08 
                           div(class="d-flex justify-content-center align-items-center gap-2 mb-4")
                             p.mb-0.f-w-500.f-12 Compared to  
                             span(class="f-light f-12 f-w-500 ") (+40.15% than)
                             p.mb-0.f-w-500.f-12 last year 
                         div(class="growth-details")
                           span(class="f-light f-12  text-uppercase") Total Purchase 
                           h4(class="f-w-500 mb-2") $42,256.26  
                           div(class="d-flex justify-content-center align-items-center gap-2 mb-4")
                             p.mb-0.f-w-500.f-12 Compared to 
                             span(class="txt-secondary f-12 f-w-500") (-20.25% than) 
                             p.mb-0.f-w-500.f-12 last year  
                         div(class="growth-details")  
                           span(class="f-light f-12  text-uppercase") Total Returns
                           h4(class="f-w-500 mb-2") $5,215.62  
                           div(class="d-flex justify-content-center align-items-center gap-2")
                             p.mb-0.f-w-500.f-12 Compared to 
                             span(class=" f-w-500 f-light f-12") (+18.15% than)  
                             p.mb-0.f-w-500.f-12 last year 
              .col-xl-4.col-md-6.box-col-none
                .row 
                  .col-md-12.col-sm-6 
                    .card(class="boost-up-card overflow-hidden")
                      div.p-4
                        div(class="boostup-name row")
                          h6(class="text-white f-28 f-w-700 mb-2 z-1 ") Boost up your sale
                          p(class="text-white f-14 f-w-500 col-9 line-clamp") by upgrading your account you can increase 
                            | your sale by 30% more.
                        div(class="img-boostup")
                          img(src='../assets/images/dashboard-3/boostup1.png' alt='boostup' class="img-boostup-img-1" )
                          img(src='../assets/images/dashboard-3/boostup2.png' alt='boostup' class='img-boostup-img-2')
                        div.btn-showcase.text-start 
                          a(href="pricing.html")
                            button.btn.btn-pill.btn-outline-light-2x.b-r-8(type='button') Upgrade Now
                  .col-md-12.col-sm-6
                    .card 
                      .card-header.card-no-border.total-revenue.pb-0
                        h4 Deliveries
                        div(class="icon-menu-header")
                          svg
                            use(href="../assets/svg/icon-sprite.svg#more-horizontal")
                      .card-body.pt-0
                        div(class="table-responsive custom-scrollbar deliveries-percentage")
                          table(class="percentage-data w-100")
                            thead  
                              tr 
                                th(scope='col' class="f-light f-12 f-w-500") Particular
                                th(scope='col' class="f-light f-12 f-w-500") Percentage
                                th(scope='col' class="f-light f-12 f-w-500 text-end") Total Amount
                            tbody 
                              tr
                                td(class="f-w-400 f-10") 
                                  a(class="line-clamp" href='cart.html') On Time Delivery
                                td 
                                  div(class="progress-value").d-flex.gap-2.align-items-center
                                    .progress
                                      .progress-bar.bg-primary(role='progressbar', style='width: 75%  ', aria-valuenow='75', aria-valuemin='0', aria-valuemax='100')
                                    span 80%
                                td(class="f-w-500 f-10 text-end") $45,452.23
                              tr
                                td(class="f-w-400 f-10") 
                                 a(class="line-clamp" href="cart.html") Delayed Delivery
                                td
                                  div(class="progress-value").d-flex.gap-2.align-items-center
                                    .progress
                                      .progress-bar.bg-primary(role='progressbar', style='width: 15%', aria-valuenow='15', aria-valuemin='0', aria-valuemax='100')
                                    span 15% 
                                td(class="f-w-500 f-10 text-end") $15,256.23
              .col-xxl-4.col-md-6.box-col-6
                .card.height-equal
                  .card-header.card-no-border.total-revenue 
                    h4 Top Product 
                    a(href="product.html") View All  
                  .card-body.pt-0
                    .top-product-card
                      +top-product(TOP_PRODUCT)
              .col-xxl-3.col-sm-6.box-col-6
                .card.height-equal
                  .card-header.card-no-border.total-revenue 
                    h4 New User  
                    a(href="product.html") View All
                  .card-body.pt-0 
                    .new-user 
                      +new-user(NEW_USER)   
              .col-xxl-5.col-sm-6.box-col-6
                .card.height-equal 
                  .card-header.card-no-border.total-revenue.pb-0 
                    h4 Team Activity 
                    a(href="product.html") View All 
                  .card-body.pt-0  
                    div(class="activity-table table-responsive custom-scrollbar")
                      include ../../components/table/activity-log-table.pug
              .col-xxl-4.col-xl-6.box-col-6 
                .card 
                  .card-header.card-no-border.total-revenue.pb-0
                    h4 User Visits by Day 
                    div.sales-chart-dropdown
                      ul.balance-data 
                        li 
                          span.circle.bg-primary
                          span.f-light.ms-1 Chrome 
                        li
                          span.circle.bg-primary-1 
                          span.f-light.ms-1 Firefox 
                        li 
                          span.circle.bg-primary-2 
                          span.f-light.ms-1 Safari
                  .card-body.pt-0.pb-0
                    .user-visitsCharts 
                      #visitsCharts 
                  .card-footer  
                    div(class="common-space") 
                      div
                        a(href="index.html" class="f-w-600 f-14 ") Most Visited Day
                        span(class="f-light f-w-500 f-14 d-block") Total 59.6k visits on Sunday 
                      div(class='visited-dropdown') 
                        svg.mb-0 
                          use(href='../assets/svg/icon-sprite.svg#arrow-down')
              .col-xxl-5.col-xl-6.box-col-6
                .card
                  .card-header.card-no-border.total-revenue 
                    h4 Latest Transaction
                    a(href="product.html") View All
                  .card-body.pt-0 
                    div(class="table-order table-responsive custom-scrollbar")
                      include ../../components/table/transaction-table.pug
              .col-xxl-3.col-xl-6.box-col-6
                .card
                  .card-header.card-no-border.total-revenue 
                    h4 Best Selling Product   
                  .card-body.pt-0
                    .selling-product
                      .apache-cotainer#echart-polar 
                      div.sales-chart-dropdown
                        ul.balance-data 
                          li 
                            span.circle.bg-primary
                            span.f-light.ms-1 Digital
                          li
                            span.circle.bg-warning
                            span.f-light.ms-1 Clothes
                          li 
                            span.circle.bg-secondary
                            span.f-light.ms-1 Electric
                          li 
                            span.circle.bg-light 
                            span.f-light.ms-1 Footwear
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
