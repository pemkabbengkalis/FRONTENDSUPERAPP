- var theme_customizer      = true;
- var apex                  = true;
- var apexmoment            = true;
- var dashboard_default     = true;
- var notify                = true;
- var animate               = true;
- var typeahead             = true;
- var typeahead_search      = true;
- var sidebar               = true; 
- var scrollbar             = true;
- var wow                   = true;
- var slick                 = true; 
- var header_slider         = true;
- var height_equal          = true;  

  
  
 
//- all mixin  
include ../../mixin/mixin 
 
doctype html
html(lang='en')
  include ../../components/header-files 
  body
    include ../../components/loader 
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body 
          .container-fluid             
            .page-title 
              .row
                .col-6
                  h4  
                    | E-commerce
                .col-6 
                  ol.breadcrumb 
                    li.breadcrumb-item 
                      a(href='index.html')
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Dashboard 
                    li.breadcrumb-item.active E-commerce
          // Container-fluid starts 
          .container-fluid
            .row 
              .col-xl-6.box-col-7 
                .card
                  .card-header.sales-chart.card-no-border.pb-0
                    h4 Sales Chart 
                    div.sales-chart-dropdown
                      ul.balance-data 
                        li 
                          span.circle.bg-warning
                          span.f-light.ms-1 Revenue
                        li
                          span.circle.bg-primary 
                          span.f-light.ms-1 Orders
                      div.sales-chart-dropdown-select
                        .card-header-right-icon.online-store
                          .dropdown
                            button#dropdownMenuButtonToggle.btn.dropdown-toggle.dropdown-toggle-store(data-bs-toggle='dropdown', aria-expanded='false' )
                              | Online Store
                            .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButtonToggle' role="menu")
                              span.dropdown-item All 
                              span.dropdown-item Employee
                              span.dropdown-item Client    
                        .card-header-right-icon 
                          .dropdown  
                            button#dropdownMenuButton1.btn.dropdown-toggle(data-bs-toggle='dropdown', aria-expanded='false')
                              | Last Year  
                            .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButton1' role="menu")
                              span.dropdown-item Last Month
                              span.dropdown-item Last Week 
                              span.dropdown-item Today  
                  .card-body.p-2.pt-0
                    .sales-wrapper
                      #saleschart 
              .col-xl-3.col-md-6.box-col-5(class="total-revenue-total-order")
                .row 
                  .col-xl-12
                    .card 
                      .card-body 
                        div.total-revenue.mb-2 
                          span Total Revenue
                          a(href="index.html") View Report
                        h3.f-w-600 ₹97,250.89 
                        div.total-chart
                            .data-grow.d-flex.gap-2
                              i.font-primary(data-feather="arrow-up-right")
                              span(class="f-w-500") 60.00% from last year 
                            .total-revenue-chart 
                              #revenue
                  .col-xl-12
                    .card 
                      .card-body 
                        div.total-revenue.mb-2  
                          span Total Order   
                          a(href="index.html") View Report
                        h3.f-w-600 35,452
                        div.total-chart
                          .data-grow.d-flex.gap-2 
                            i.font-secondary(data-feather="arrow-down-left")
                            span(class="f-w-500") 15.00% from last week
                          .total-order
                            #totalOrder
              .col-xl-3.col-md-6(class="special-Offer-banner").box-col-none
                .card
                  div(class="special-Offer")
                    div(class="offer-contain")
                      h4 Today’s Special Offer
                      p.mt-2.text-center You can flat get 20% off on your next pro version if your sale break your last record.
              .col-xxl-6.box-col-12 
                .row
                  .col-xl-5.col-sm-6
                    .card.height-equal
                      .card-body 
                        +product-cost(PRODUCT_COSTING)
                  .col-xl-7.col-sm-6
                    .card.height-equal
                      .card-header.pb-0.total-revenue.card-no-border 
                          h4 Sale History 
                          a(href="index.html") View All
                      .card-body  
                        +sale-history(SALE_HISTORY) 
              .col-xxl-6.col-md-12.box-col-12 
                .card.height-equal
                  .card-header.total-revenue.card-no-border
                      h4 Latest Orders
                      div.d-flex.align-items-center.gap-2
                        span.update-data.d-none.d-md-block.f-light Data updates in every 3 hours
                        .sales-chart-dropdown-select
                          .card-header-right-icon 
                            .dropdown 
                              button#dropdownMenuButtondownMenu.btn.dropdown-toggle(data-bs-toggle='dropdown', aria-expanded='false')
                                | Today 
                              .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButtondownMenu' role="menu")
                                span.dropdown-item Last Month
                                span.dropdown-item Last Week 
                                span.dropdown-item Last Day
                  .card-body.pt-0 
                    .table-order.table-responsive.custom-scrollbar    
                      include ../../components/table/orders-table.pug 
              .col-xxl-6.box-col-12
                .card  
                  .card-header.total-revenue.card-no-border  
                      h4 Live Product
                      a(href="product.html") View All
                  .card-body.pt-0
                    div(class="table-order table-responsive custom-scrollbar")
                      include ../../components/table/live-product.pug
              .col-xxl-3.col-md-6.box-col-6
                .card
                  .card-header.total-revenue.card-no-border
                      h4 Top Revenue Product
                      div.sales-chart-dropdown-select
                        .card-header-right-icon
                          .dropdown
                            button#dropdownMenuButtonup.btn.dropdown-toggle(data-bs-toggle='dropdown', aria-expanded='false' )
                              | Today
                            .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButtonup' role="menu")
                              span.dropdown-item Last Month
                              span.dropdown-item Last Week  
                              span.dropdown-item Last Day 
                  .card-body.pt-0  
                    div(class="revenueproduct" id="revenueproduct") 
                    div.sales-chart-dropdown 
                        ul.balance-data.flex-wrap.flex
                          li
                            span.circle.bg-primary
                            span.f-light.ms-1 Men & Women Jeans
                          li
                            span.circle.bg-warning
                            span.f-light.ms-1 Men & Women T-shirts
                          li
                            span.circle.bg-secondary
                            span.f-light.ms-1 Men & Women Shoes
                          li
                            span.circle.bg-light
                            span.f-light.ms-1 Kurtas & Kurti
              .col-xxl-3.col-md-6.box-col-6
                .card 
                    .div(class='ecommerce-banner') 
                .col-xl-12
                  .card(class="product-widget")
                    .card-body(class="new-product")
                        div(class="product-cost")
                          div(class="add-product")
                            div(class='product-icon bg-light-primary')
                              svg 
                                use(href="../assets/svg/icon-sprite.svg#box-add")
                            div 
                              h6.mb-1 Add New Product
                              span.f-light Images are crucial showcasing
                          div(class='product-icon')
                            svg
                              use(href="../assets/svg/icon-sprite.svg#arrow-down")
                .col-xl-12 
                  .card(class="product-widget") 
                    .card-body(class="new-product")  
                        div(class="product-cost")  
                          div(class="add-product")
                            div(class='product-icon bg-light-primary')
                              svg 
                                use(href="../assets/svg/icon-sprite.svg#receipt-disscount")
                            div
                              h6.mb-1 Add Disscount  
                              span.f-light The product images to the platform
                          div(class='product-icon')  
                            svg 
                              use(href="../assets/svg/icon-sprite.svg#arrow-down")

          // Container-fluid Ends 
        include ../../components/footer
    include ../../components/footer-files 
    script. 
      new WOW().init();